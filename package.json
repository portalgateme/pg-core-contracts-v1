{
  "name": "pg-contracts-v1",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prepare": "husky install",
    "compile": "npx hardhat compile --force",
    "coverage": "npx hardhat coverage",
    "test": "npx hardhat test",
    "test-all": "npm run deploy-localhost && npm run test",
    "private-network": "npx hardhat node --no-deploy",
    "private-network-deploy": "npx hardhat node",
    "deploy:goerli": "npx hardhat deploy --network goerli --tags stage",
    "deploy:localhost": "npx hardhat deploy --network localhost",
    "verify:goerli": "npx hardhat --network goerli etherscan-verify",
    "lint-fix": "npm run lint-sol-fix && npm run lint-js-fix && npm run lint-json-fix && npm run lint-scripts-tests",
    "lint-json-fix": "prettier --write \"./[a-zA-Z0-9.]+(?!-lock).json\"",
    "lint-js-fix": "prettier --write \"./**/*.js\"",
    "lint-sol-fix": "prettier --write \"contracts/**/*.sol\"",
    "lint-scripts-tests": "prettier --write \"./scripts/**/*.ts\" && prettier --write \"./test/**/*.ts\" && eslint . --ext .ts --fix",
    "generate-types": "TYPECHAIN_FORCE=true npx hardhat typechain",
    "circuit:reward": "scripts/buildCircuit.sh Reward",
    "circuit:withdraw": "scripts/buildCircuit.sh Withdraw",
    "circuit:treeUpdate": "scripts/buildCircuit.sh TreeUpdate",
    "circuit": "mkdir -p build/circuits && yarn circuit:reward && yarn circuit:withdraw && yarn circuit:treeUpdate"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@ensdomains/ens": "^0.6.2",
    "@ensdomains/ens-contracts": "^0.0.20",
    "@ensdomains/eth-ens-namehash": "^2.0.15",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-toolbox": "^2.0.2",
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@^0.3.0-beta.13",
    "@nomiclabs/hardhat-etherscan": "^3.0.0",
    "@nomiclabs/hardhat-ganache": "^2.0.1",
    "@openzeppelin/contracts-upgradeable": "^4.9.0",
    "@openzeppelin/hardhat-upgrades": "^1.22.1",
    "@typechain/ethers-v5": "^10.2.0",
    "@typechain/hardhat": "^8.0.0",
    "@types/chai": "^4.3.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "chai": "^4.3.7",
    "circomlib": "git+https://github.com/tornadocash/circomlib.git#3b492f9801573eebcfe1b6c584afe8a3beecf2b4",
    "circom": "0.0.35",
    "dotenv": "^16.0.3",
    "eslint": "^8.40.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "ethereumjs-util": "^7.1.5",
    "ethers": "^5.7.2",
    "hardhat": "^2.14.0",
    "hardhat-deploy": "^0.11.29",
    "hardhat-gas-reporter": "^1.0.8",
    "husky": "^8.0.3",
    "prettier-plugin-solidity": "^1.1.3",
    "solhint": "^3.4.1",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.8.1",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.1",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "decimal.js": "^10.2.0",
    "eth-sig-util": "^2.5.3",
    "fixed-merkle-tree": "^0.3.4",
    "snarkjs": "git+https://github.com/tornadocash/snarkjs.git#869181cfaf7526fe8972073d31655493a04326d5",
    "web3": "^1.2.11",
    "websnark": "git+https://github.com/tornadocash/websnark.git#86a526718cd6f6f5d31bdb1fe26a9ec8819f633e"
  }
}
